					"datalakemain21_accountKey": {
						"value": "[parameters('datalakemain21_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStorageShar_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageShar_properties_typeProperties_url')]"
					},
					"datalakecicd_properties_typeProperties_url": {
						"value": "[parameters('datalakecicd_properties_typeProperties_url')]"
					},
					"datalakemain21_properties_typeProperties_url": {
						"value": "[parameters('datalakemain21_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sharcog21_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
