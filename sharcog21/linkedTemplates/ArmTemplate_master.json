		},
		"AzureDataLakeStorageShar_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shardatalake.dfs.core.windows.net/"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Student"
		},
		"datalakecicd_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shardatalake.dfs.core.windows.net/"
		},
		"datalakemain21_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shardatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sharcog21_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_servicePrincipalCredential": {
						"value": "[parameters('AzureDataLakeStorage1_servicePrincipalCredential')]"
