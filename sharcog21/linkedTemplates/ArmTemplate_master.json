					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sharcog21_ArmTemplate_0')]"
			]
		},
		{
			"name": "sharcog21_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sharcog21_ArmTemplate_1')]"
			]
		}
	]
}